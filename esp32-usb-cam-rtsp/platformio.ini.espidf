; ESP-IDF Framework version (backup)
; Bu dosyayı kullanmak için platformio.ini olarak yeniden adlandırın

[env:esp32cam]
platform = espressif32@6.9.0
framework = espidf
board = esp32cam
monitor_speed = 115200
board_build.partitions = partitions.csv

build_flags = 
  -DCONFIG_SPIRAM_SUPPORT=1
  -DCONFIG_ESP32_SPIRAM_SUPPORT=1
